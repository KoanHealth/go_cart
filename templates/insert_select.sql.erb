-- Transformation from <%= from_table.symbol %> to <%= to_table.symbol %>
-- Originally generated by go_cart
<% to_table_max = to_table.fields.size - 1 %>
INSERT INTO <%= to_table.symbol %> (
<% to_table.fields.each_with_index do |pair, index| %>    <%= pair.first %><%= index == to_table_max ? '' : ',' %>
<% end %>)
SELECT
<% mapping.each_with_index do |pair, index| %>    <%= pair.last.nil? ? 'null' : "src.#{pair.last}" %> as <%= pair.first %><%= index == to_table_max ? '' : ',' %>
<% end %>/*
<% from_table.fields.each do |symbol, field| %>    src.<%= symbol %>
<% end %>*/
FROM <%= from_table.symbol %> src
